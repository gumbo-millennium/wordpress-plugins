{
  "name": "@gumbomillennium/plugin",
  "version": "1.0.0",
  "description": "Gumbo Millennium WordPress Plugin",
  "repository": "git@gitlab.com:gumbo-millennium/plugin.git",
  "contributors": [
    {
      "name": "Roelof Roos",
      "email": "github@roelof.io",
      "url": "https://github.com/roelofr"
    }
  ],
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "build": "yarn run production",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "start": "yarn run development --watch",
    "watch": "yarn run development --watch",
    "watch-poll": "yarn run watch --watch-poll"
  },
  "devDependencies": {
    "babel-plugin-sass-vars": "^0.2.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "cleave.js": "^1.3.7",
    "cross-env": "^5.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "laravel-mix": "^2.0",
    "lodash": "^4.17.11",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1",
    "stylelint-webpack-plugin": "^0.10.5"
  }
}
